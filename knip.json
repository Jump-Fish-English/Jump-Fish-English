{
  "workspaces": {
    "packages/astro-rpc": {
      "ignoreDependencies": [
        "@babel/preset-typescript"
      ]
    },
    "packages/animation-player": {
      "vite": {
        "config": ["vite.config.{js,mjs,ts,cjs,mts,cts}"]
      },
      "entry": [
        "playground.ts"
      ]
    },
    "packages/studio": {
      "astro": {
        "entry": [
          "astro.config.{js,cjs,mjs,ts}",
          "src/content/config.ts",
          "src/pages/**/*.{astro,mdx,js,ts}",
          "src/content/**/*.mdx"
        ]
      },
      "entry": [
        "src/components/Main.tsx"
      ]
    },
    "packages/video-processor": {
      "entry": [
        "playground.ts"
      ],
      "vite": {
        "config": ["vite.config.{js,mjs,ts,cjs,mts,cts}"]
      }
    }
  }
  
}